<?php
/**
 * @file
 * Install file.
 */

/**
 * Implements hook_enable().
 */
function required_by_role_enable() {
  variable_set('field_widget_required_callback', 'required_by_role_field_widget_is_required');
}

/**
 * Implements hook_disable().
 */
function required_by_role_disable() {
  variable_set('field_widget_required_callback', 'field_widget_is_required');
}
